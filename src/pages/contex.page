<apex:page standardController="Contact">
<apex:form >
<apex:pageBlock title="Contact Info" mode="edit">
<apex:pageBlockButtons >

   
<apex:commandButton action="{!save}" value="Save"/>

</apex:pageBlockButtons>

<apex:pageBlockSection title="Contact Details" columns="1" collapsible="false">

<apex:inputField value="{!contact.Birthdate}"/>
<apex:inputField value="{!contact.MobilePhone}"/>
<apex:inputField value="{!contact.Email}"/>
</apex:pageBlockSection>
<apex:pageblockSection title="Address Information" columns="1" collapsible="false">
<apex:inputField value="{!contact.HomePhone}"/>
<apex:inputField value="{!contact.CleanStatus}"/>
<apex:inputField value="{!contact.LastCUUpdateDate}"/>
</apex:pageblockSection>
</apex:pageBlock>
</apex:form>  
</apex:page>